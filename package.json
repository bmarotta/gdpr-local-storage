{
  "name": "gdpr-local-storage",
  "version": "1.0.0",
  "description": "GDPR Local Storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmarotta/gdpr-local-storage.git"
  },
  "keywords": [
    "GDPR",
    "TCF API",
    "Local Storage",
    "Cookie"
  ],
  "author": "Bruno Marotta",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bmarotta/gdpr-local-storage/issues"
  },
  "homepage": "https://github.com/bmarotta/gdpr-local-storage#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
